<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/sobot_photo_bg"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/sobot_rl_gif"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:visibility="gone">

        <com.sobot.workorder.weight.image.SobotGifView
            android:id="@+id/sobot_image_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerInParent="true" />
    </RelativeLayout>

    <com.sobot.widget.image.photoview.SobotPhotoView
        android:id="@+id/sobot_order_big_photo"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_centerInParent="true"
        android:visibility="gone" />

    <com.sobot.workorder.weight.image.SoborRoundProgressBar
        android:id="@+id/sobot_pic_progress_round"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_centerInParent="true" />
</RelativeLayout>